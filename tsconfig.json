{
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "target": "es2020",
    "lib": [
      "esnext",
      "webworker",
      "es2020"
    ],
    "strict": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "types": [
      "@cloudflare/workers-types",
			"service-worker"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ]
}
